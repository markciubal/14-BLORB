{{#each posts as |post| }}
<div class="row align-center">
  <div>
    <h2>{{{get_emoji}}} {{post.title}}</h2>
  </div>
  <div>
    <p>{{post.body}}</p>
  </div>
  <div>
    <h7>By: {{post.user.name}} @ {{post.createdAt}}</a>
      {{#ifEquals post.id user_id}}
      {{/ifEquals}}
    </h7>

  </div>
</div>
{{/each}}